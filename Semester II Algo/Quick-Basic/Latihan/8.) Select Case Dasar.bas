CLS
DIM x AS STRING

INPUT "Masukan nomor"; x
SELECT CASE x
    CASE "a"
        PRINT "kamu memilih nomor 1"
    CASE "b"
        PRINT "kamu memilih nomor 2"
    CASE "c"
        PRINT "kamu memilih nomor 3"
    CASE "d"
        PRINT "kamu memilih nomor 4"
    CASE "e"
        PRINT "kamu memilih nomor 5"
    CASE "f"
        PRINT "kamu memilih nomor 6"
END SELECT


